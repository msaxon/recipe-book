{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["plugin:prettier/recommended"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "no-console": "warn"
  }
}